{
  "name": "json-path-finder",
  "displayName": "JSON Path Finder",
  "description": "Visual JSON path finder with interactive tree view - like findjsonpath.com",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "publisher": "amaankulshreshtha",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/json-path-finder.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "jsonPathFinder.openViewer",
        "title": "Open JSON Path Finder",
        "category": "JSON Path Finder"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == json",
          "command": "jsonPathFinder.openViewer",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "jsonPathFinder.openViewer",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j",
        "when": "resourceLangId == json"
      }
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  }
}
